<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-4.2.xsd">
    <context:component-scan base-package="rocketwash.testautomation"/>

    <bean id="springTest" class="webdriver.ExtendedWebDriverFactoryBean" />
    <bean id="loginPage" class="pages.LoginPage" scope="prototype" init-method="initElements"/>
    <bean id="homePage" class="pages.HomePage" scope="prototype" init-method="initElements"/>
    <bean id="basePage" class="pageobjectutils.Page" scope="prototype" init-method="initElements"/>
</beans>